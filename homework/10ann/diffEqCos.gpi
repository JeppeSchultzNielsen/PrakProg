set terminal svg
set output "Plots/DiffEqCos.svg"
set key top right
set xlabel "x"
set ylabel "y"
set tics out
set title "Solution to u''-u=0 from 0 to 2pi with y(pi)=0 and y'(pi)=1 with 7 neurons"
plot "txts/diffEqCos.txt" using ($1):($2) with lines title "y"\
,"txts/diffEqCos.txt" using ($1):($3) with lines title "y'"\
,"txts/diffEqCos.txt" using ($1):($4) with lines title "y''"\