set terminal svg
set output "Plots/DiffEqSin.svg"
set key top right
set xlabel "x"
set ylabel "y"
set tics out
set title "Solution to u''-u=0 from 0 to 2pi with y(0)=0 and y'(0)=1 with 7 neurons"
plot "txts/diffEqSin.txt" using ($1):($2) with lines title "y"\
,"txts/diffEqSin.txt" using ($1):($3) with lines title "y'"\
,"txts/diffEqSin.txt" using ($1):($4) with lines title "y''"\