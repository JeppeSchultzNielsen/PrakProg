set terminal svg
set output "Plots/Linterp.svg"
set key top right
set xlabel "x"
set ylabel "y"
set tics out
set title "Linear"
plot "txts/sinData.txt" using 1:2 with points title "10 points of Sin(x)"\
,"txts/linterp.txt" using ($1):($2) with lines lt rgb "black" title "Linear interpolation"\
,"txts/linterpInteg.txt" using ($1):($2) with lines lt rgb "black" title "Integral of linear interpolation (1-cos(x))"\

