set terminal svg
set output "Plots/Higgs.svg"
set key top right
set xlabel "E [GeV]"
set ylabel "Signal"
set tics out
set title "Breit-Wigner fit to Higgs signal"
plot "txts/fittedData.dat" using ($1):($2) with lines lt rgb "black" title "Breit-Wigner fit"\
,"txts/higgs.data" using 1:2:3 with yerrorbars title "Signal"\

